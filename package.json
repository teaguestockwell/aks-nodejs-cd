{
  "name": "node-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for node.js REST API",
  "license": "MIT",
  "author": {
    "name": "Teague Stockwell",
    "email": "tsappdevelopment@gmail.com",
    "url": "https://teaguestockwell.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/teaguestockwell/node-boilerplate"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "compile": "tsc --noEmit",
    "build": "webpack",
    "docker:build": "docker build -t node-rest .",
    "docker:run": "docker run -e secret=c2VjcmV0 -it -p 3000:3000 node-rest",
    "docker:start": "yarn docker:build && yarn docker:run",
    "bench": "chmod +x ./src/index.bench.sh && ./src/index.bench.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npx tsc --noEmit"
    }
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.4",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-jest": "^28.1.3",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.4",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
